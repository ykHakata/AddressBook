    <div class="list-group">
    % for my $word (@{$search_list}) {
      <a href="#" class="list-group-item clearfix">
        <img src="<%= config 'url_public' %>/img/<%= $word->{icon} %>" class="img-responsive img-thumbnail pull-left" style="width: 50px; margin-right: 15px;">
        <h4 class="list-group-item-heading"><%= $word->{title} %></h4>
        <p class="list-group-item-text">sample text</p>
      </a>
    % }
    </div>
